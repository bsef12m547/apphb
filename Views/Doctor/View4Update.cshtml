@model BSEF12m547.Models.Doctor
@{
    ViewBag.Title = "View4Update";
    Layout = "~/Views/Shared/_Layout3.cshtml";
}
<html>

<body>

    <!--/. NAV TOP  -->
    <!-- /. NAV SIDE  -->
    <div id="page-wrapper">
        <div id="page-inner">


            <div class="row">
                <div class="col-md-12">
                    <h1 class="page-header">
                        Edit <small> Your information</small>
                    </h1>
                    <div class="jumbotron vertical-center">


                        <div class="container text-center">
                            <form action="@Url.Action("UpdateOccure", "Doctor" )" method="Post" enctype="multipart/form-data" class="col-sm-12">
                                <div class="form-group">
                                    <div class="col-xs-2"></div>
                                    <div class="pull-left">
                                        <input type="file" name="image" />
                                    </div>
                                </div>

                                < <br /> <br />


                                <input class="center-block" style="width:200px ; visibility:hidden" type="text" value="@Model.Id" name="Id">
                                <input class="center-block" style="width:200px ; visibility:hidden" type="text" value="@Model.NoOfAppointments" name="NoOfAppointments">
                                <input class="center-block" style="width:200px ; visibility:hidden" type="text" value="@Model.longitude" name="longitude">
                                <input class="center-block" style="width:200px ; visibility:hidden" type="text" value="@Model.latitude" name="latitude" >

                                <div class="form-group">
                                    <div class="col-xs-2"></div>

                                    <div class="pull-left">
                                        <label for="exampleInput1" class="col-sm-6 control-label">Name</label>
                                    </div>
                                    <br /><br />
                                    <div class="col-xs-2"></div>
                                    <div class="col-xs-6">

                                        <input type="text" name="Name" class="form-control" id="exampleInput1" value="@Model.Name">

                                    </div>
                                </div>
                                <br /> <br />


                                <div class="form-group">
                                    <div class="col-xs-2"></div>
                                    <div class="pull-left">
                                        <label for="exampleInput2" class="col-sm-6 control-label">Email</label>
                                    </div><br /><br />
                                    <div class="col-xs-2"></div>
                                    <div class="col-sm-6">
                                        <input type="text" name="Email" class="form-control" id="exampleInput2" value="@Model.Email">
                                    </div>

                                </div>
                                <br /> <br />
                                <div class="form-group">
                                    <div class="col-xs-2"></div>
                                    <div class="pull-left">
                                        <label for="exampleInput3" class="col-sm-6 control-label">Password</label>
                                    </div><br /><br />
                                    <div class="col-xs-2"></div>
                                    <div class="col-sm-6">
                                        <input type="password" name="Password" class="form-control" id="exampleInput3" value="@Model.Password">
                                    </div>
                                </div>
                                <br /> <br />

                                <div class="form-group">
                                    <div class="col-xs-2"></div>
                                    <div class="pull-left">
                                        <label for="exampleInput1" class="col-sm-6 control-label">City</label>
                                    </div>
                                    <div>
                                        <label for="exampleInput1" class="col-sm-6 control-label">Address</label>
                                    </div>
                                    <br /><br />

                                    <div class="col-xs-2"></div>
                                    <div class="col-xs-3">
                                        <input type="text" name="City" class="form-control" id="exampleInput1" value="@Model.City">
                                    </div>
                                    <div class="col-xs-3">
                                        <input type="text" name="Address" class="form-control" id="exampleInput2" value="">
                                <!--       <input type="tel" name="Telephone_" class="form-control" id="exampleInput2" value="Model.Telephone_"> --> 
                                    </div>
                                </div>
                                <br /> <br />

                                <div class="form-group">
                                    <div class="col-xs-2"></div>
                                    <div class="pull-left">
                                        <label for="exampleInput3" class="col-sm-6 control-label">Specialised_In</label>
                                    </div><br /><br />
                                    <div class="col-xs-2"></div>
                                    <div class="col-sm-6">
                                        <input type="text" name="SpecialiseIn" class="form-control" id="exampleInput3" value="@Model.SpecialiseIn">
                                    </div>
                                </div>
                                <br /> <br />

                                <div class="form-group">
                                    <div class="col-xs-2"></div>
                                    <div class="pull-left">
                                        <label for="exampleInput1" class="col-sm-6 control-label">Clinic</label>
                                    </div>
                                    <div>
                                        <label for="exampleInput1" class="col-sm-6 control-label">Hospital</label>
                                    </div>
                                    <br /><br />

                                    <div class="col-xs-2"></div>
                                    <div class="col-xs-3">
                                        <input type="text" name="Clinic" class="form-control" id="exampleInput1" value="@Model.Clinic">
                                    </div>
                                    <div class="col-xs-3">
                                        <input type="tel" name="Hospital" class="form-control" id="exampleInput2" value="@Model.Hospital">
                                    </div>
                                </div>
                                <br /> <br />

                                <div class="form-group">
                                    <div class="col-xs-2"></div>
                                    <div class="pull-left">
                                        <label for="exampleInput3" class="col-sm-6 control-label">Birthday</label>

                                    </div>
                                    <div>
                                        <label for="exampleInput1" class="col-sm-6 control-label">Telephone</label>
                                    </div>
                                    <br /><br />
                                    <div class="col-xs-2"></div>
                                    <div class="col-xs-3">
                                        <input type="date" name="month" class="form-control" id="exampleInput1" placeholder="Pick DOB" value="@Model.month" >
                                    </div>
                                    <div class="col-xs-3">
                                        <input type="tel" name="Telephone_" class="form-control" id="exampleInput2" placeholder="Telephone" value="@Model.Telephone_">
                                    </div>

                                    <br /> <br />
                                    <div class="form-group">
                                        <div class="col-xs-2"></div>
                                        <div class="pull-left">
                                            <label for="exampleInput3" class="col-sm-6 control-label">Appointment_Fee</label>
                                        </div><br /><br />
                                        <div class="col-xs-2"></div>
                                        <div class="col-sm-6">
                                            <input type="number" name="AppointmentFee" class="form-control" id="exampleInput3" value="@Model.AppointmentFee">
                                        </div>
                                    </div>
                                </div>
                                    <br /> <br />



                                    <div class="form-group">
                                        <div class="col-xs-2"></div>
                                        <div class="pull-left">
                                            <label for="exampleInput3" class="col-sm-6 control-label">Gender</label>
                                        </div><br /><br />
                                        <div class="col-xs-2"></div>
                                        <div class="col-sm-6">
                                            <select class="form-control" id="exampleInput3" name="sex">
                                                <option value="male">Male</option>
                                                <option value="female">Female</option>
                                            </select>
                                        </div>
                                    </div>

                                    <br />
                                    <br />
                                    <br />
                                    <div class="col-xs-2"></div>
                                    <div style="font:100;color:black" class="pull-left">
                                        <input type="submit" name="submit" value="Submit " style="background: #409ba1" class="btn btn-default" />
                                    </div>
</form>

                        </div>

                    </div>

                </div>
            </div>
            <footer>
                <p>All right reserved <a href="http://webthemez.com">Mehreen</a></p>
            </footer>
        </div>
        <!-- /. PAGE INNER  -->
    </div>





    <br /><br />

</body>

</html>
